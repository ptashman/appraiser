<p id="notice"><%= notice %></p>

<h1>Listing Census Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @census_records.each do |census_record| %>
      <tr>
        <td><%= link_to 'Show', census_record %></td>
        <td><%= link_to 'Edit', edit_census_record_path(census_record) %></td>
        <td><%= link_to 'Destroy', census_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Census record', new_census_record_path %>
